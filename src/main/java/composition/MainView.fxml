<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.MainControl">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="280.0" prefWidth="400.0">
         <center>
            <TableView prefHeight="200.0" prefWidth="200.0">
              <columns>
                  <TableColumn prefWidth="75.0" text="Compte" />
                <TableColumn prefWidth="75.0" text="Nom" />
                <TableColumn prefWidth="75.0" text="Prenom" />
              </columns>
            </TableView>
         </center>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
      <BorderPane layoutX="414.0" layoutY="-24.0">
         <top>
            <Label text="Simple" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="motButton" mnemonicParsing="false" onAction="#onMotClicked" text="randomize" GridPane.columnIndex="1" />
                  <Button fx:id="nbButton" mnemonicParsing="false" onAction="#onNbClicked" text="mutliply" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="motLabel" />
                  <TextField fx:id="nbLabel" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
